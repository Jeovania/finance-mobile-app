{
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint src -c .eslintrc.js --ext ts,tsx,js,jsx",
    "pretest": "jest --clearCache",
    "prettify": "prettier --check src/**/*.{js,ts,jsx,tsx} --write",
    "send:coverage:report": "scripts/./send_coverage_report.sh",
    "test": "jest --runInBand",
    "test:changed": "jest -o",
    "test:ci": "jest --ci --runInBand --collectCoverage",
    "test:coverage": "jest --coverage --colors",
    "test:staged": "jest --ci --findRelatedTests",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.5",
    "@react-navigation/native": "^5.1.3",
    "@react-navigation/stack": "^5.2.8",
    "expo": "~36.0.0",
    "expo-font": "~8.0.0",
    "expo-localization": "~8.0.0",
    "i18next": "^19.3.4",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-i18next": "^11.3.4",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-reanimated": "~2.10.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-svg": "9.13.3",
    "react-native-web": "~0.11.7",
    "styled-components": "^5.0.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^25.1.4",
    "@types/react": "~16.9.0",
    "@types/react-native": "~0.60.23",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "~8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "jest-expo": "^36.0.1",
    "lint-staged": "^10.0.9",
    "react-test-renderer": "^16.13.1",
    "typescript": "~3.6.3"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,md,json}": [
      "yarn lint",
      "yarn prettify",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
